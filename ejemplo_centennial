global _start

section .text

_start:   
    ; write(int fd, const void *buf, size_t count)    
    xor    eax,eax    
    xor    ebx,ebx    
    xor    ecx,ecx        
    xor    edx,edx    
    mov    al,0x4    
    inc    bl    
    push   0x000a2164    
    push   0x6c726f57    
    push   0x202c6f6c        
    push   0x6c6548    
    mov    ecx,esp    
    mov    dl,0xf    
    int    0x80
    
a. nasm -f elf32 -o code-one.o code-one.asm

b. ld -o code-one code-one.o

c. chmod +x code-one

d. ./code-one


for i in $(find / -executable -type f);do file -i $i | grep -i 'x-executable; charset=binary';done.
